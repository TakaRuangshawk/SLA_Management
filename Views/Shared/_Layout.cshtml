<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SLA_Management</title>
       <!-- Style -->
   @* <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/MainCSS_gateway.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/SLA_Management.styles.css" asp-append-version="true" />
    <link rel="shortcut icon" href="~/images/favicon.ico" type="image/x-icon"/>
    <script src="~/js/jquery-3.3.1.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>*@
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/MainCSS_gateway.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />


    <link href="~/lib/bootstrap-select.1.13.18/content/content/bootstrap-select.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-select.1.13.18/content/content/bootstrap-select.min.css" rel="stylesheet" />

    <link href="~/lib/bootstrap-select.1.13.18/css/bootstrap-select.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/SLA_Management.styles.css" asp-append-version="true" />

    <script src="~/lib/jquery/dist/jquery.min.js"></script>


    <script src="~/lib/popper.js/popper.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/lib/bootstrap-select.1.13.18/js/bootstrap-select.js"></script>
</head>
<style>
    .navbar {
            padding-top: 0px;
            padding-bottom: 0px;
            border: 0;
            border-radius: 0;
            margin-bottom: 0;
            font-size: 14px;
            letter-spacing: 1px;
            background-color: #f3cb9b;
    }
    .navbar a:hover, .dropdown:hover .dropbtn {
           background-color: #997e5d;
    }
    .nav-link{
            padding: 1rem 1rem;
    }
</style>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <div>
                     <img src="~/images/2.png" width="127" height="53" alt="Logo" align="left" style="padding-right:30px;" >
                </div>
                
                @*<a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">SLA Management</a>*@
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                       <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
                                Monitor
                            </a>
                            <div class="dropdown-menu">
                                <a class="dropdown-item text-dark" asp-area="" asp-controller="EJAddTranProbTerm" asp-action="EJAddTranProbTermAction">DeviceTermProb</a>
                                <a class="dropdown-item text-dark" asp-area="" asp-controller="Operation" asp-action="SlaTracking">Sla Tracking</a>
                                <a class="dropdown-item text-dark" asp-area="" asp-controller="Gateway" asp-action="Transaction">Gateway Transaction</a>
                            </div>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
                                Maintenance
                            </a>
                            <div class="dropdown-menu">
                                <a class="dropdown-item text-dark" asp-area="" asp-controller="Comlog" asp-action="Index">InsertFileCOMLog</a>
                                <a class="dropdown-item text-dark" asp-area="" asp-controller="ComlogNew" asp-action="Index">InsertFileCOMLogNew</a>
                            </div>
                        </li>
                          <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
                                Report
                            </a>
                            <div class="dropdown-menu">
                                <a class="dropdown-item text-dark" asp-area="" asp-controller="Operation" asp-action="SlaReportDaily">SlaReportDaily</a>
                                <a class="dropdown-item text-dark" asp-area="" asp-controller="Operation" asp-action="SlaReportMonthly">SlaReportMonthly</a>
                            </div>
                        </li>               
                        @* <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="EJAddTranProbTerm" asp-action="EJAddTranProbTermAction">DeviceTermProb</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Comlog" asp-action="Index">Comlog Manage</a>
                        </li>*@
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container-fluid" style = "background-color:#e3e3e3">
            Copyright&copy; 2023 - Dataone Asia Co., Ltd
        </div>
    </footer>
    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>*@
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
